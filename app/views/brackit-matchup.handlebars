

<div id="play">
    {{>color-background}}
    {{>header}}
    {{>question-header}}
    <h5 class="question brackit-question title-text">{{candidates.0.question}}</h5>
    <p class="primary-text body-pos-6">tap to select or switch between choices, then hit vote when youâ€™re ready!</p>
    <h6 class="choice cand1 cand-1 matchup-cand-a primary-text" data-id="{{candidates.0.candidateId}}">{{candidates.0.candidateName}}</h6>
    <h6 class="choice cand2 cand-2 matchup-cand-b primary-text" data-id="{{candidates.1.candidateId}}">{{candidates.1.candidateName}}</h6>
    <button class="pick-cand brackit-blue vote-btn primary-text">vote</button>
    <p class="timer">5</p>
    <p class="coral-text round-pos">round[<span class="blue-text" id="round-num" data-num="{{candidates.0.roundNumber}}">{{candidates.0.roundNumber}}</span>]</p>

