{{>color-background}}
{{>header}}
{{>question-header}}

<div id="play">

    <h5 class="question brackit-question title-text">{{candidates.0.question}}</h5>
    <h6 class="choice cand1 cand-1" data-id="{{candidates.0.candidateId}}">{{candidates.0.candidateName}}</h6>
    <h6 class="choice cand2 cand-2" data-id="{{candidates.1.candidateId}}">{{candidates.1.candidateName}}</h6>
    <button class="pick-cand">brack[it]!</button>
    <p class="timer">5</p>

</div>

<p class="coral-text directions-pos-5">round[<span class="blue-text" id="round-num" data-num="{{candidates.0.roundNumber}}">{{candidates.0.roundNumber}}</span>]</p>